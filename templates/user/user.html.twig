{#
/**
 * @file
 * Cats theme implementation to present user data.
 *
 * Available variables:
 * - content: A list of content items.
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_user()
 */
#}
<article{{ attributes.addClass('profile') }}>
  {% if content %}
    {{ content }}
  {% endif %}
</article>
